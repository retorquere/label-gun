    "@octokit/request-error": "^3.0.2",
    "@octokit/rest": "^19.0.5",
    "@octokit/webhooks": "^10.3.1",
    "@octokit/webhooks-definitions": "^3.65.6",
    "fast-safe-stringify": "^2.1.1",
    "rools": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.15",
    "@typescript-eslint/parser": "^5.46.1",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.29.0",
